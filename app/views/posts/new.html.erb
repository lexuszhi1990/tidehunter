<%= render 'shared/content_lead', header: "New Post" %>
<div class="normal-form">
  <%= render 'shared/errors', obj: @post %>
  <%= form_for @post do |f| %>
    <%= f.label :title %>
    <%= f.text_field "title" %>
    <%= f.label :tag %>
    <%= f.text_field "tag" %>
    <%= f.label :body %>
    <%= f.text_area "body", rows: 4 %>
    <br/>
    <%= f.submit :mypost, class: "btn btn-success" %>
  <% end %>
</div>
